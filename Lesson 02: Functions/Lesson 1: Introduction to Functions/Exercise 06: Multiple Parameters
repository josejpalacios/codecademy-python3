# Changed function name to mult_x_add_y
# Added x and y as parameters
def mult_x_add_y(number, x, y):
  # Changed 2 to x and 3 to y
  print(number*x + y)
  
# Call mult_x_add_y with:
# number:5
# x:2
# y:3
mult_x_add_y(5, 2, 3)

# Call mult_x_add_y with:
# number:1
# x:3
# y:1
mult_x_add_y(1, 3, 1)
